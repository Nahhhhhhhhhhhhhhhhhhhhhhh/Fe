-- Zen Hub Fe Bypass with Close (X) and Zen Fe Toggle Button

local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "ZenHubFeBypass"
ScreenGui.ResetOnSpawn = false
ScreenGui.Parent = game.CoreGui

-- Main Frame
local Frame = Instance.new("Frame")
Frame.Size = UDim2.new(0, 400, 0, 350)
Frame.Position = UDim2.new(0.3, 0, 0.2, 0)
Frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
Frame.BorderSizePixel = 0
Frame.Active = true
Frame.Draggable = true
Frame.Visible = true
Frame.Parent = ScreenGui

-- Title
local Title = Instance.new("TextLabel")
Title.Size = UDim2.new(1, -40, 0, 40)
Title.Position = UDim2.new(0, 0, 0, 0)
Title.BackgroundTransparency = 1
Title.Text = "Zen Hub Fe Bypass"
Title.Font = Enum.Font.SourceSansBold
Title.TextSize = 26
Title.TextColor3 = Color3.fromRGB(255, 50, 50)
Title.TextXAlignment = Enum.TextXAlignment.Left
Title.Parent = Frame

-- X (Close) Button
local CloseButton = Instance.new("TextButton")
CloseButton.Size = UDim2.new(0, 40, 0, 40)
CloseButton.Position = UDim2.new(1, -40, 0, 0)
CloseButton.Text = "X"
CloseButton.Font = Enum.Font.SourceSansBold
CloseButton.TextSize = 22
CloseButton.TextColor3 = Color3.fromRGB(255, 255, 255)
CloseButton.BackgroundColor3 = Color3.fromRGB(100, 30, 30)
CloseButton.Parent = Frame

-- Scrolling Buttons
local ScrollingFrame = Instance.new("ScrollingFrame")
ScrollingFrame.Size = UDim2.new(1, -10, 1, -50)
ScrollingFrame.Position = UDim2.new(0, 5, 0, 45)
ScrollingFrame.CanvasSize = UDim2.new(0, 0, 10, 0)
ScrollingFrame.ScrollBarThickness = 5
ScrollingFrame.BackgroundTransparency = 1
ScrollingFrame.Parent = Frame

local UIListLayout = Instance.new("UIListLayout")
UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
UIListLayout.Padding = UDim.new(0, 5)
UIListLayout.Parent = ScrollingFrame

-- Zen Fe Toggle Button
local ToggleButton = Instance.new("TextButton")
ToggleButton.Size = UDim2.new(0, 100, 0, 35)
ToggleButton.Position = UDim2.new(0, 10, 0, 10)
ToggleButton.Text = "Zen Fe"
ToggleButton.Font = Enum.Font.SourceSansBold
ToggleButton.TextSize = 18
ToggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
ToggleButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
ToggleButton.Visible = false
ToggleButton.Parent = ScreenGui

-- Hide and Show Functionality
CloseButton.MouseButton1Click:Connect(function()
	Frame.Visible = false
	ToggleButton.Visible = true
end)

ToggleButton.MouseButton1Click:Connect(function()
	Frame.Visible = true
	ToggleButton.Visible = false
end)

-- Function to Add Script Buttons
local function createButton(name, url)
	local btn = Instance.new("TextButton")
	btn.Size = UDim2.new(1, -10, 0, 35)
	btn.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
	btn.TextColor3 = Color3.fromRGB(255, 255, 255)
	btn.Font = Enum.Font.SourceSans
	btn.TextSize = 18
	btn.Text = name
	btn.BorderSizePixel = 0
	btn.Parent = ScrollingFrame

	btn.MouseButton1Click:Connect(function()
		local success, err = pcall(function()
			loadstring(game:HttpGet(url))()
		end)
		if not success then
			warn("Failed to load:", err)
		end
	end)
end

-- Example Scripts
createButton("Grab Knife V1", "https://pastebin.com/raw/SSAzNj10")
createButton("Grab Knife V2", "https://pastebin.com/raw/Bw7aWTBL")
createButton("Grab Knife V3", "https://pastebin.com/raw/frHvA27v")
createButton("Grab Knife V4", "https://pastebin.com/raw/ANs9UDrn")
createButton("Coolkid V4", "rbxassetid://157645868")
createButton("Coolkid V3", "rbxassetid://142756431")
createButton("Ro-Xploit 6.0", "https://pastebin.com/raw/qqgpugGF")
createButton("666 GUI", "https://pastebin.com/raw/d56eg7r9")
createButton("Lua Hammer", "https://pastebin.com/raw/RKY5fTXh")
createButton("Bird Wings", "https://pastebin.com/raw/zCypYHjJ")
createButton("Nebula Hub", "https://pastebin.com/raw/fHGzkgyU")
createButton("Virgo V3", "https://pastebin.com/raw/EwYDRD4Y")
createButton("Apoc Tools V4", "https://pastebin.com/raw/0de4tAtR")
