-- Load UI Library
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Zen Hub - FD Expansion", "BloodTheme")

--// FD SCRIPTS TAB \\--
local fdTab = Window:NewTab("FD Scripts")
local knifeSec = fdTab:NewSection("Grab Knife")
local roxSec = fdTab:NewSection("Ro-Xploit")
local coolSec = fdTab:NewSection("Coolkid")
local otherSec = fdTab:NewSection("Other Tools")

-- Helper for buttons
local function newButton(section, title, url, useGetObject)
    section:NewButton(title, "Click to run " .. title, function()
        pcall(function()
            local scriptToLoad
            if useGetObject then
                scriptToLoad = game:GetObjects(url)[1].Source
            else
                scriptToLoad = game:HttpGet(url, true)
            end
            loadstring(scriptToLoad)()
        end)
    end)
end

-- Knife Versions
newButton(knifeSec, "Grab Knife V1", "https://pastebin.com/raw/SSAzNj10")
newButton(knifeSec, "Grab Knife V2", "https://pastebin.com/raw/Bw7aWTBL")
newButton(knifeSec, "Grab Knife V3", "https://pastebin.com/raw/frHvA27v")
newButton(knifeSec, "Grab Knife V4", "https://pastebin.com/raw/ANs9UDrn")

-- Coolkid
newButton(coolSec, "Coolkid V3", "rbxassetid://142756431", true)
newButton(coolSec, "Coolkid V4", "rbxassetid://157645868", true)

-- Ro-Xploit
newButton(roxSec, "Ro-Xploit 4.0", "rbxassetid://175137115", true)
newButton(roxSec, "Ro-Xploit 5.0", "rbxassetid://288646117", true)
newButton(roxSec, "Ro-Xploit 6.0", "https://pastebin.com/raw/qqgpugGF")

